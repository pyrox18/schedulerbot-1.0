  "bugs": {
    "url": "https://github.com/pyrox18/schedulerbot/issues"
  },
  "homepage": "https://github.com/pyrox18/schedulerbot",
  "dependencies": {
    "chrono-node": "~1.3.5",
    "dotenv": "~4.0.0",
    "eris": "~0.8.2",
    "fuzzyset.js": "0.0.3",
    "moment-timezone": "~0.5.14",
    "mongoose": "~4.11.14",
    "node-schedule": "~1.2.5",
    "raven": "~2.2.1",
    "redis": "~2.8.0",
    "redislock": "~1.3.0",
    "winston": "~2.3.1",
    "winston-mongodb": "~3.0.2"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.2",
    "@types/moment-timezone": "^0.5.2",
