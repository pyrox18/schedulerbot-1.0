    "@types/node-schedule": "^1.2.1",
    "@types/raven": "^2.1.2",
    "@types/redis": "^2.8.3",
    "@types/winston": "^2.3.7",
    "concurrently": "^3.5.1",
    "gulp": "^3.9.1",
    "nodemon": "^1.13.3",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  }
